# Database Configuration (Cloud SQL PostgreSQL)
DATABASE_URL=postgresql://user:password@localhost:5432/esferazap

# Google Cloud Storage
GCS_BUCKET_NAME=esferazap-media
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Google Gemini AI
GEMINI_API_KEY=your_gemini_api_key

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Server Configuration
PORT=8080
NODE_ENV=production

# Frontend URL (Firebase Hosting)
FRONTEND_URL=https://esferazap.web.app
CORS_ORIGIN=https://esferazap.web.app

# OAuth (opcional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# WhatsApp Session Storage
WHATSAPP_SESSION_PATH=/app/auth_sessions

